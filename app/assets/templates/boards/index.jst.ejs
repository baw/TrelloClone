<h1>Boards</h1>

<ul>
  <% _(boards).each(function (board) { %>
    <li>
      <a href="#/board/<%= board.escape('id') %>"><%= board.escape("title") %></a>
      <button data-boardid="<%= board.escape('id') %>"
              class="btn btn-danger deleteBoard">
        Delete
      </button>
    </li>
  <% }) %>
</ul>

<div class="newBoard"></div>